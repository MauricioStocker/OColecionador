package oColecionador.entity;

import javax.persistence.FetchType;
import javax.persistence.JoinColumn;
import javax.persistence.ManyToOne;

import lombok.ToString;

public class NotaProdutoColecaoId {
	
	
    @ToString.Exclude
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "id_colecao", nullable = false, referencedColumnName = "id_colecao")
    private ColecaoEntity colecaoEntity;

    
    @ToString.Exclude
    @ManyToOne(fetch = FetchType.LAZY)
    @JoinColumn(name = "id_nota_produto", nullable = false, referencedColumnName = "id_nota_produto")
    private NotaProdutoEntity notaProdutoEntity;

}
